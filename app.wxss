/**app.wxss**/
@import "libs/weui.wxss";

page {
    background-color: #fff;
    box-sizing: border-box;
}

swiper {
    width: 100%;
}

swiper image {
    width: 100%;
}

.container {
    height: 100%;
    width: 100%;
    display:flex;
    flex-direction:column;
    align-items:center;/*垂直居中*/
    justify-content: center;/*水平居中*/
    box-sizing: border-box;
}

.ui-main {
    box-sizing: border-box;
}