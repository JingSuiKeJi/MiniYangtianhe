/* tabBar总体 */
.tab-bar{
	position: fixed;
	left: 0;
	/* bottom:25rpx; */
	bottom:22rpx;
	width: 100%;
	height: 136rpx;
	display: flex;
	flex-direction: row;
}
/* 背景图片 */
.tab-bar-bg{
  display: flex;
  position: absolute;
  width: 100%;
  height: 136rpx;
  z-index: -1;
}
/* tabBar内容快 */
.tab-bar-index{
  display: flex;
  flex: 1;
  font-size: 20rpx;
  height:100%;
  margin-top:50rpx;
  z-index: 1;
}
/* tarBar养乐步单独内容快 */
.tab-bar-index-add{
  display: flex;
  flex: 1;
  font-size: 26rpx;
  z-index: 1;
  height: 100%;
}
/* 图标字体结合内容快 */
.tab-bar-icon{
	width: 100%;
	height: 86rpx;
	display: flex;
	flex-direction: column;
	align-items: center;
	justify-content: space-between;
}
/* 图标字体结合内容快添加发布单独设置 */
.tab-bar-icon_add{
  width: 100%;
  height: 100%;
  display: flex;
  flex-direction: column;
}
/* 内容快图片 */
.tab-bar-pharmacyImg{
	width: 36rpx;
	height: 44rpx;
	margin: 0 auto;
}
.tab-bar-marketImg{
	width: 44rpx;
	height: 44rpx;
	margin: 0 auto;
}
.tab-bar-cartImg{
	width: 48rpx;
	height: 44rpx;
	margin: 0 auto;
}
.tab-bar-meImg{
	width: 34rpx;
	height: 44rpx;
	margin: 0 auto;
}
/* 内容快添加发布单独图片设置 */
.tab-bar-img-add{
	width: 92rpx;
	height: 92rpx;
	margin: 10rpx auto 0 auto;
}
.tab-bar-add-text{
	font-size:20rpx;
	font-family:PingFangSC;
	font-weight:500;
	text-align: center;
	margin: 10rpx auto 0 auto;
}
/* 内容快文字 */
.tab-bar-text{
  text-align: center;
  font-size: 20rpx;
  margin: 0 auto;
}

