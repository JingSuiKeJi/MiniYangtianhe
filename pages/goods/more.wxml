                </view>
                <view class="ui-item_teamer">还差<text >1</text>人成团</view>
                <view class="ui-item_btnBox" bindtap="onJoinTap">
                    <image src="{{filter.getLocalPhoto('goCollageIcon.png')}}" class="ui-btnItem_btn"/>
                    <text class=".ui-btnItem_text">去参团</text>
                </view>
            </view>
        </view>
    </view>
</view>
