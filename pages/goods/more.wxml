<view class="ui-contain">
    <view class="main">
        <view class="moreBox">
            <view class="moreItem">
                <view class="ui-item_imgBox">
                    <image src="../../images/touxiang.png"  class="ui-imgItem_icon"/>
                </view>
                <view class="ui-item_info">
                    <view class="ui-infoItem_title">15677220282的团购</view>
