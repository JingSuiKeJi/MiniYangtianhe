/* pages/store/cart.wxss */

page {
    width: 100%;
    background-color: #F5F5F5;
}
.main {
    position: relative;
}
.main__box {
	position: relative;
    width: 100%;
}

.tab-bottom{
	position: fixed;
	z-index: 9999999;
	left: 0;
	bottom: 0;
	width: 100%;
	height: 25rpx;
	background:#F9F9F9;
}

.ui-headerBox {
    width: 100%;
    height: 62rpx;
    line-height: 62rpx;
    text-align: center;
    background-color: #FFF0F1;
}

.headerItem_labaIcon {
    width: 26rpx;
    height: 26rpx;
}

.headerItem_text {
    font-size: 24rpx;
    font-family: PingFang-SC-Medium;
    font-weight: 500;
    color: #F6959C;
    line-height: 24rpx;
    margin: 20rpx;
    display: inline-block;
    vertical-align: top;
}

.headerItem_rightIcon {
    width: 12rpx;
    height: 18rpx;
    display: inline-block;
    vertical-align: top;
    margin-top: 24rpx;
}

.ui-contentBox {
    width: 100%;
    background-color: #fff;
}

.ui-contentItem__tips {
    width: 100%;
    height: 80rpx;
    line-height: 76rpx;
    position: relative;
}

.ui-tipsItem__selectIcon {
    width: 40rpx;
    height: 40rpx;
    vertical-align: middle;
    margin-left: 30rpx;
    margin-right: 30rpx;
}

.ui-tipsItem__storeIcon {
    width: 30rpx;
    height: 30rpx;
    vertical-align: middle;
    margin-right: 30rpx;
}

.ui-tipsItem__storeName {
    font-size: 30rpx;
    font-family: PingFangSC-Semibold;
    font-weight: 600;
    color: #333333;
    line-height: 30rpx;
}

.ui-tipsItem__text {
    position: absolute;
	z-index: 999;
    right: 30rpx;
    top: 58rpx;
}

.ui-contentItem__list {
    width: 100%;
}

.ui-contentItem__listBox {
    width: 100%;
    padding-bottom: 10rpx;
    position: relative;
    white-space: nowrap;
    overflow: hidden;
    border-top: 1rpx solid #F5F5F5;
}

.ui-contentItem__listItem {
    display: inline-block;
    width: 100%;
    height: 100%;
    position: relative;
    margin-right: 0;
    transition: all 0.4s;
    transform: translateX(110rpx);
    margin-left: -110rpx
}

.ui-listItem__selectIcon {
    width: 40rpx;
    height: 40rpx;
    margin: 13% 30rpx;
    vertical-align: top;
}

.ui-goodsBox {
    display: inline-block;
}

.ui-listItem__goodsIcon {
    width: 160rpx;
    height: 160rpx;
    margin-top: 40rpx;
    margin-bottom: 40rpx;
}

.ui-listItem__info {
    display: inline-block;
    vertical-align: top;
    margin-left: 30rpx;
}

.ui-infoItem__title {
    width: 400rpx;
    font-size: 26rpx;
    font-family: PingFangSC;
    font-weight: 500;
    color: #333333;
    line-height: 36rpx;
    margin-top: 50rpx;
    white-space: normal;
}

.ui-infoItem__scale {
    width: 400rpx;
    font-size: 20rpx;
    font-family: PingFang-SC;
    font-weight: 400;
    color: #999999;
    line-height: 20rpx;
    margin-top: 38rpx;
    margin-bottom: 20rpx;
    white-space: normal;
}

.ui-infoItem__price {
    position: relative;
    width: 400rpx;
}

.ui-infoItem__price>text {
    font-size: 26rpx;
    font-family: PingFangSC;
    font-weight: 500;
    color: #ED3D3B;
    line-height: 26rpx;
    white-space: normal;
}

.ui-listItem__del {
    width: 120rpx;
    height: 100%;
    padding-top: 13%;
    background-color: #20CAB4;
    display: inline-block;
    color: #fff;
    font-size: 24rpx;
    font-family: PingFangSC;
    font-weight: 600;
    text-align: center;
    transform: translateX(120rpx);
    transition: all 0.4s;
    position: absolute;
}

.touch-move-active .ui-contentItem__listItem,
.touch-move-active .ui-listItem__del {
    -webkit-transform: translateX(0);
    transform: translateX(0);
}

.ui-listItem__operatebBox {
    display: inline-block;
    position: absolute;
    top: 10rpx;
    right: 0;
}

.ui-listItem__operatebBox>image {
    width: 40rpx;
    height: 40rpx;
}

.operateItem__text {
    display: inline-block;
    margin: 9rpx 20rpx;
    line-height: 26rpx;
    font-size: 26rpx;
    font-family: PingFangSC;
    font-weight: 500;
    color: #333333;
    vertical-align: top;
}

.ui-storeBox {
    margin-top: 20rpx;
    margin-bottom: 260rpx
}

.ui-fixBox {
    position: fixed;
    bottom: 120rpx;
    margin-top: 100rpx;
    padding-top: 20rpx;
    padding-bottom: 56rpx;
    width: 100%;
    background-color: #fff;
    box-sizing: border-box;
}

.ui-fixItem__selectIcon {
    width: 40rpx;
    height: 40rpx;
    vertical-align: top;
    margin: 33rpx 15rpx auto 30rpx;
}

.ui-fixItem__text {
    font-size: 26rpx;
    font-family: PingFang-SC;
    font-weight: 400;
    color: #333333;
    line-height: 26rpx;
    display: inline-block;
    vertical-align: top;
    margin: 40rpx 130rpx auto 20rpx;
}

.ui-fixItem__allText {
    font-size: 28rpx;
    font-family: PingFang-SC;
    font-weight: 500;
    color: #666666;
    line-height: 28rpx;
    display: inline-block;
    vertical-align: top;
    margin-top: 37rpx;
}

.ui-fixItem__price {
    font-size: 28rpx;
    font-family: PingFangSC;
    font-weight: 600;
    color: #ED3D3B;
    line-height: 28rpx;
    display: inline-block;
    vertical-align: top;
    margin-top: 37rpx;
    white-space: normal;
}

.ui-fixItem__btn {
    display: inline-block;
    width: 192rpx;
    height: 90rpx;
    line-height: 90rpx;
    margin-left: 20rpx;
    font-size: 30rpx;
    font-family: PingFangSC;
    font-weight: 600;
    color: #fff;
    /* background-image: url(http://www.gzlingren.com:8098/yangtianhe/cart_greenBtn.png);
    background-size: 192rpx 90rpx; */
    background-color: #3ED8C8;
    border-radius: 50rpx;
    text-align: center;
    position: absolute;
    top: 7rpx;
    right: 20rpx;
}

.ui-textBox {
    margin-left: 20rpx;
}

.ui-fixItem__delectBtn {
    position: absolute;
    right: 20rpx;
    top: 40rpx;
    display: inline-block;
    width: 148rpx;
    height: 60rpx;
    /* line-height:60rpx; */
    font-size: 30rpx;
    font-family: PingFangSC;
    font-weight: 400;
    color: #FF5F55;
    text-align: center;
    border-radius: 30rpx;
    border: 2px solid #FF5F55;
}

.ui-noBox {
    /* text-align: center; */
    position: relative;
    width: 100%;
    height: 100%;
    display: flex;
    align-items: center;
    justify-content: center;
    flex-direction: column;

}

.ui-cartIcon {
    width: 118rpx;
    height: 118rpx;
    margin-top: 124rpx;
}

.ui-noBox>view {
    font-size: 30rpx;
    font-family: PingFangSC;
    font-weight: 400;
    color: #999999;
    line-height: 42rpx;
}

.ui-text {
    margin-top: 40rpx;
}

.ui-noLogin__text {
    font-size: 30rpx;
    font-family: PingFangSC;
    font-weight: 400;
    color: #666666;
    line-height: 30rpx;
    margin-top: 400rpx;
}

.ui-loginBtn {
    display: flex;
    width: 206rpx;
    height: 72rpx;
    font-size: 30rpx;
    font-family: PingFangSC;
    font-weight: 400;
    color: #fff;
    background-color: #53DBD4;
    border-radius: 10rpx;
    justify-content: center;
    align-items: center;
    margin-top: 50rpx;
}
.ui-topListBox {
    width: 100%;
    background-color: #F5F5F5;
    padding: 0 32rpx 260rpx 32rpx;
}
.ui-topListItem__tip {
    display: flex;
    align-items: center;
    justify-content: center;
    height: 132rpx;
}
.ui-tipItem__text {
    font-size:32rpx;
    font-family:PingFangSC-Regular,PingFang SC;
    font-weight:400;
    color:rgba(51,51,51,1);
    margin-left: 24rpx;
    margin-right: 24rpx;
}
.ui-topListitem__goods {
    display: flex;
    flex-wrap: wrap;
}
.ui-goodsItem {
    width:332rpx;
    height:498rpx;
    margin-bottom: 20rpx;
    margin-right: 22rpx;
    background:rgba(255,255,255,1);
    border-radius:12rpx;
}
.ui-goodsItem:nth-child(2n) {
    margin-right: 0rpx;
}
.ui-goodsItem__img {
    width:332rpx;
    height:322rpx;
}
.ui-goodsItem > view {
    width:290rpx;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
    margin-left: 24rpx;
}
.ui-goodsItem__title {
    width:290rpx;
    height:24rpx;
    margin: 24rpx 18rpx 24rpx 24rpx ;
    font-size:24rpx;
    font-family:PingFangSC-Regular,PingFang SC;
    font-weight:400;
    color:rgba(61,61,61,1);
    line-height:24rpx;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
}
.ui-goodsItem__scale {
    height:20rpx;
    font-size:20rpx;
    font-family:PingFangSC-Regular,PingFang SC;
    font-weight:400;
    color:rgba(153,153,153,1);
    line-height:20rpx;
}
.ui-goodsItem__price {
    margin-top: 22rpx;

}
.ui-goodsItem__nowPrice {
    display: inline-block;
    vertical-align: top;
    font-size:24rpx;
    font-family:PingFangSC-Regular,PingFang SC;
    font-weight:400;
    color:rgba(255,95,85,1);
    line-height:36rpx;
}
.ui-goodsItem__prePrice {
    margin-left: 20rpx;
    font-size:24rpx;
    font-family:PingFangSC-Regular,PingFang SC;
    font-weight:400;
    color:rgba(204,204,204,1);
    line-height:36rpx;
    text-decoration: line-through;
    display: inline-block;
    vertical-align: top;
}
.ui-tipItem__firstLine, .ui-tipItem__secondLine{
    display: line-block;
    width: 54rpx;
    height: 3rpx;
    background-color: #333333;
    position: relative;
}
.ui-tipItem__firstLine::after {
    display: inline-block;
    position: absolute;
    right: 0;
    top: -2rpx;
    content: '';
    width: 8rpx;
    height: 8rpx;
    background-color: #333333;
    border-radius: 50%;
}
.ui-tipItem__secondLine::before {
    display: inline-block;
    position: absolute;
    left: 0;
    top: -2rpx;
    content: '';
    width: 8rpx;
    height: 8rpx;
    background-color: #333333;
    border-radius: 50%;
}
.ui-noData__word {
    font-size: 26rpx;
    font-family: PingFangSC-Regular,PingFangSC;
    font-weight: 400;
    color: #999999;
    line-height: 50rpx;
    text-align: center;

}
.ui-topListItem__noData {
    font-size: 30rpx;
    font-family: PingFangSC-Regular,PingFang SC;
    font-weight: 400;
    color: #999999;
    line-height: 50rpx;
    text-align: center;

}