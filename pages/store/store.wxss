    font-weight: normal;
}

.ui-collageScroll_itemPre {
    width: 160rpx;
    font-size: 26rpx;
    font-family: PingFangSC-Regular;
    font-weight: 400;
    color: #999999;
    line-height: 26rpx;
    text-decoration: line-through;
    text-align: left;
    margin: 0 auto;
}

.ui-bottomBox {
    width: 100%;
    height: 108rpx;
    text-align: center;
    line-height: 108rpx;
    font-size: 24rpx;
    font-family: PingFangSC-Regular;
    font-weight: 400;
    color: #999999;
    background-color: #fff;
}

.ui-headBox {
    width: 100%;
    padding-top: 12rpx;
