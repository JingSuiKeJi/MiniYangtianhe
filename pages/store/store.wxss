    position: relative;
}

.ui-collageItem_top .ui-topItem_title {
    display: inline-block;
    font-size: 32rpx;
    font-family: PingFangSC-Medium;
    font-weight: 500;
    color: #333333;
}

.ui-collageItem_top .ui-topItem_more {
    height: 100rpx;
    display: inline-block;
    font-size: 22rpx;
    font-family: PingFangSC-Semibold;
    font-weight: 600;
    color: #BBBBBB;
    position: absolute;
    right: 0;
}

.ui-topItem_more .ui-topItem_icon {
    display: inline-block;
    width: 24rpx;
    height: 24rpx;
    margin-top: 38rpx;
    vertical-align: top;
}

.ui-collageItem_bottom {
    width: 100%;
    height: 424rpx;
}

