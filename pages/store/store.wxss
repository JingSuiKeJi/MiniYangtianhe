    margin: 15rpx auto;
}

.ui-collageScroll_itemTitle {
    width: 160rpx;
    height: 68rpx;
    font-size: 28rpx;
    font-family: PingFangSC-Regular;
    font-weight: 400;
    color: #333333;
    line-height: 34rpx;
    margin: 0 auto;
    white-space: pre-wrap;
    text-align: left;
    -webkit-line-clamp: 2;
    display: -webkit-box;
    -webkit-box-orient: vertical;
    overflow: hidden;
    text-overflow: ellipsis;
}

.ui-collageScroll_itemNow {
    width: 160rpx;
    font-size: 24rpx;
    font-family: PingFangSC-Regular;
    font-weight: 400;
