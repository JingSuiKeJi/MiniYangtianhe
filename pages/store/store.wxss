}

.ui-noticeBox {
    width: 100%;
    min-height: 70rpx;
    padding: 15rpx 30rpx;
    margin: 0 auto;
    line-height: 50rpx;
    background-color: #fff;
}

.ui-noticeBox>text {
    display: inline-block;
    font-size: 24rpx;
    font-family: PingFangSC-Regular;
    font-weight: 400;
    color: #777777;
    margin-right: 37rpx;
}

.ui-noticeBox>text:first-child {
    color: #FF4035;
