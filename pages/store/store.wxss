/* pages/store/store.wxss */

page {
    background-color: #F5F5F5;
}


.container {
    height: 100%;
    width: 100%;
    background-color: #F5F5F5;
}

.main {
    width: 100%;
    /*margin-bottom: 166rpx;*/
