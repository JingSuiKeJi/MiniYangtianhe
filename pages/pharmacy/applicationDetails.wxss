@charset "UTF-8";
@import "/libs/common.wxss";

/**applicationDetails.wxss**/
page{
	background:rgba(255,255,255,1);
}

.ui-main {
    width: 100%;
	display: flex;
	flex-direction: column;
	align-items: center; /*垂直方向居中*/
}

.ui-withdrawDepositTitle{
	height:26rpx;
	font-size:26rpx;
	font-family:PingFangSC-Regular;
	font-weight:400;
	color:rgba(51,51,51,1);
	line-height:26rpx;
	margin-top: 44rpx;
}

.ui-cashWithdrawalAmount{
	height:56rpx;
	font-size:56rpx;
	font-family:PingFangSC-Medium;
	font-weight:500;
	color:rgba(51,51,51,1);
	line-height:56rpx;
	margin-top: 40rpx;
}

.ui-currentStatus{
	height:34rpx;
	font-size:34rpx;
	font-family:PingFangSC-Regular;
	font-weight:400;
	color:rgba(255,170,0,1);
	line-height:34rpx;
	margin-top: 20rpx;
	margin-bottom: 86rpx;
}

.ui-currentStatusUnderway{
	color:rgba(255,170,0,1);
}

.ui-currentStatusSuccess{
	color:rgba(153,153,153,1);
}

.ui-transaction{
	width: 670rpx;
	display: flex;
	justify-content: space-between;
	margin-bottom:40rpx;
}

.ui-transaction text:first-child{
	height:30rpx;
	font-size:30rpx;
	font-family:PingFang-SC-Regular;
	font-weight:400;
	color:rgba(153,153,153,1);
	line-height:30rpx;
}

.ui-transaction text:last-child{
	height:30rpx;
	font-size:30rpx;
	font-family:PingFang-SC-Regular;
	font-weight:400;
	color:rgba(51,51,51,1);
	line-height:30rpx;
}

.ui-processingProgress{
	width: 430rpx;
	display: flex;
	flex-direction: column;
}

.ui-processingProgressItems{
	display: flex;
}

.ui-processingProgressItems text:nth-child(2){
	height:26rpx;
	font-size:26rpx;
	font-family:PingFang-SC-Regular;
	font-weight:400;
	color:rgba(32,202,180,1);
	line-height:26rpx;
	margin-left: 32rpx;
}

.ui-processingProgressItems text:nth-child(3){
	height:22rpx;
	font-size:22rpx;
	font-family:PingFang-SC-Regular;
	font-weight:400;
	color:rgba(153,153,153,1);
	line-height:22rpx;
	position: absolute;
	right: 40rpx;
	margin-top: 4rpx;
}

.ui-processingProgressItems:nth-child(3) text:nth-child(2){
	margin-top: 5rpx;
}

.ui-processingProgressItems:nth-child(3) text:nth-child(3){
	margin-top: 9rpx;
}

.ui-processingProgressItems:nth-child(5) text:nth-child(2){
	margin-top: 10rpx;
	color:rgba(153,153,153,1);
}

.ui-processingProgressItems:nth-child(5) text:nth-child(3){
	margin-top: 14rpx;
}

.ui-withdrawalSuccessText{
	color: #20CAB4;
}

.ui-progress{
	width:4rpx;
	height:44rpx;
	margin-left: 16rpx;
}

.ui-progressBarSuccess{
	background:rgba(29,196,188,1);
}

.ui-progressBarNot{
	background:rgba(225,225,225,1);
}

.ui-progressBarNotHeight{
	height:46rpx;
}

.ui-successIcon{
	width: 36rpx;
	height: 36rpx;
	background-image:url('http://www.gzlingren.com:8098/yangtianhe/my_workedOut.png');
	background-repeat: no-repeat;
	background-position: center center;
	background-size: cover;
}

.ui-notSuccessIcon{
	width: 36rpx;
	height: 36rpx;
	background-image:url('http://www.gzlingren.com:8098/yangtianhe/my_ processed.png');
	background-repeat: no-repeat;
	background-position: center center;
	background-size: cover;
}

