	margin-left: 32rpx;
}

.ui-withdrawDetail{
	width:622rpx;
	height:96rpx;
	background:rgba(255,255,255,1);
	display: flex;
	justify-content: center;
	align-items: center;
	border-bottom:#F5F5F5 solid 2rpx;
	color:rgba(102,102,102,1);
}

.ui-withdrawDetail text{
	width: 100%;
	height:36rpx;
	font-size:26rpx;
	font-family:PingFangSC-Regular;
	font-weight:400;
	line-height:36rpx;
}

.ui-withdrawDetail text:nth-child(2){
	text-align: center;
	height:40rpx;
	font-size:28rpx;
	line-height:40rpx;
}

.ui-withdrawDetail text:nth-child(3){
	text-align: right;
}

/* 模态框 */
.modal-mask {
	width: 100%;
	height: 100%;
	position: fixed;
	top: 0;
	left: 0;
	background:rgba(0,0,0,1);
	opacity: 0.5;
	overflow: hidden;
	z-index: 9000;
	color: #fff;
}
.modal-dialog {
	width:540rpx;
	height:208rpx;
	background:rgba(255,255,255,0.9);
	border-radius:16rpx;
	overflow: hidden;
	position: fixed;
	top: 50%;
	left: 0;
	z-index: 9999;
	margin: -180rpx 105rpx;
}
.modal-title {
	margin-top: 36rpx;
	margin-left:30rpx ;
	width:480rpx;
	height:48rpx;
	font-size:34rpx;
	font-family:PingFangSC-Semibold;
	font-weight:600;
	color:rgba(0,0,0,1);
	line-height:48rpx;
	text-align: center;
}

.modal-footer {
	display: flex;
	flex-direction: row;
	height: 86rpx;
	border-top: 1px solid #dedede;
	font-size: 34rpx;
	line-height: 86rpx;
	margin-top: 34rpx;
}
.btn-cancel {
	width: 50%;
	font-size:34rpx;
	font-family:PingFang-SC-Medium;
	font-weight:500;
	color:rgba(0,122,255,1);
	text-align: center;
	border-right: 1px solid #dedede;
}
.btn-confirm {
	width: 50%;
	font-size:34rpx;
	font-family:PingFangSC-Semibold;
	font-weight:600;
	color:rgba(253,61,47,1);
	text-align: center;
}