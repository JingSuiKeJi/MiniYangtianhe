@charset "UTF-8";
@import "/libs/common.wxss";

/**messageInform.wxss**/
page{
	background:#FFFFFF;
}

.ui-main {
    width: 100%;
}

.ui-commonBox{
	width: 718rpx;
	height: 92rpx;
	border-bottom:rgba(0,0,0,0.1) solid 2rpx;
	margin-left: 32rpx;
	display: flex;
	justify-content: space-between;
	align-items: center;
}

.ui-commonBox text{
	margin-left: 8rpx;
	height:30rpx;
	font-size:30rpx;
	font-family:PingFang-SC-Medium;
	font-weight:500;
	color:rgba(51,51,51,1);
	line-height:30rpx;
}

.ui-commonBox view{
	width: 104rpx;
	height: 60rpx;
	margin-right: 40rpx;
	background-repeat: no-repeat;
	background-position: center center;
	background-size: cover;
}

.ui-Open{
	background-image:url('https://app.yth-yx.com/mini/my_switchOn.png');
}

.ui-Close{
	background-image:url('https://app.yth-yx.com/mini/my_switchOff.png');
}